    "import socket\n",
    "\n",
    "client_socket = socket.socket(socket.AF_INET, socket.SOCK_STREAM)\n",
    "\n",
    "server_address = ('localhost', 12330)\n",
    "client_socket.connect(server_address)\n",
    "\n",
    "data = client_socket.recv(1024)\n",
    "print(data.decode('utf-8'))\n",
    "\n",
    "client_socket.close()"
   ]
  }
